{
    "$schema": "http://json-schema.org/draft-04/schema",

    "title": "Schema for a Service Basics",
    "type": "object",

    "required": ["protocol_binding", "message_bindings"],

    "properties": {
        "protocol_binding": {
            "type": "string"
        },
        "message_bindings": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "minItems": 1,
            "uniqueItems": true
        }
    }
}


