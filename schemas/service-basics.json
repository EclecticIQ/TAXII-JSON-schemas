{
    "$schema": "http://json-schema.org/draft-04/schema",

    "title": "Schema for a Service Basics",
    "type": "object",

    "required": ["protocol", "encodings"],

    "properties": {
        "protocol": {
            "type": "string"
        },
        "encodings": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "minItems": 1,
            "uniqueItems": true
        }
    }
}


